<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToPadding="true"
    android:contextClickable="true"
    android:fadeScrollbars="true"
    android:longClickable="true"
    tools:context=".FacialwashActivity">

    <ImageView
        android:id="@+id/facialwash"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/facialwash"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <Button
        android:id="@+id/cosrxbutton"
        android:layout_width="418dp"
        android:layout_height="166dp"
        android:background="?android:attr/selectableItemBackground"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/facialwash"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.615"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/facialwash"
        app:layout_constraintVertical_bias="0.225"
        app:rippleColor="#FFFFFF" />
</androidx.constraintlayout.widget.ConstraintLayout>